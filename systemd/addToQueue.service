[Unit]
Description=Add pubkeys to queue for processing
After=network.target

[Service]
Type=simple
User=hasenpfeffr
EnvironmentFile=/etc/hasenpfeffr.conf
ExecStart=${HASENPFEFFR_NODE_BIN} /usr/local/lib/node_modules/hasenpfeffr/src/pipeline/stream/addToQueue.mjs
Restart=on-failure
RestartSec=1

[Install]
WantedBy=multi-user.target