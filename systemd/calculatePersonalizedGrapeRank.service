[Unit]
Description=calculate personalizedGrapeRank and store scores in Neo4j

[Service]
Type=oneshot
User=hasenpfeffr
EnvironmentFile=/etc/hasenpfeffr.conf
ExecStart=/usr/local/lib/node_modules/hasenpfeffr/src/algos/personalizedGrapeRank/calculatePersonalizedGrapeRank.sh

[Install]
WantedBy=multi-user.target
